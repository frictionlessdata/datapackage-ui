{
  "name": "datapackage-ui",
  "version": "1.0.1",
  "license": "MIT",
  "author": "roll <eskarev@gmail.com>",
  "description": "UI components for datapackage",
  "homepage": "https://github.com/frictionlessdata/datapackage-ui",
  "bugs": "https://github.com/frictionlessdata/datapackage-ui/issues",
  "repository": "github:frictionlessdata/datapackage-ui",
  "main": "dist/datapackage-ui.js",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "build": "npm run compile && npm run bundle",
    "bundle": "webpack --output-pathinfo --progress --hide-modules && NODE_ENV=production webpack --progress --hide-modules",
    "compile": "babel src --out-dir lib",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "dev": "webpack-dev-server --inline --hot",
    "format": "prettier --write '{src,test}/**/*.js' && eslint --fix '{src,test}/**/*.js'",
    "lint": "prettier --check '{src,test}/**/*.js' && eslint '{src,test}/**/*.js'",
    "pretest": "npm run compile && npm run lint",
    "readme": "doctoc --maxlevel 3 README.md",
    "test": "npm run test:node && npm run test:browser",
    "test:node": "NODE_ENV=testing nyc mocha",
    "test:browser": "NODE_ENV=testing karma start"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "classnames": "^2.2.5",
    "datapackage": "^1.0.6",
    "lodash": "^4.17.4",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "regenerator-runtime": "^0.11.0",
    "tableschema": "^1.8.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.4",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.1.2",
    "chromedriver": "^2.27.2",
    "compression-webpack-plugin": "^0.3.2",
    "coveralls": "^2.13.1",
    "css-loader": "^3.4.2",
    "doctoc": "^1.4.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.0.5",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "http-server": "^0.11.1",
    "husky": "^3.0.8",
    "jsdom": "^15.1.1",
    "karma": "^1.6.0",
    "karma-jsdom-launcher": "^8.0.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.2.4",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^2.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "prettier": "^2.0.2",
    "react-hot-loader": "^4.12.20",
    "react-test-renderer": "^16.1.1",
    "referencer": "^0.2.5",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.8.2"
  }
}
